<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


                                                            <!-- how we inject property by autoWiring-->
    <bean id="logger" class="se.joshua.autowiring.Logger"  autowire="byType">
    </bean>

    <bean id="fileWriter" class="se.joshua.autowiring.FileWriter"/>
    <bean id="consoleWriter" class="se.joshua.autowiring.ConsoleWriter"/>


</beans>