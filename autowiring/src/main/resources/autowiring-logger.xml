<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


                                                            <!-- how we inject property by autoWiring by type-->
    <bean id="loggerByType" class="se.joshua.spring.autowiring.LoggerByType" autowire="byType">
    </bean>

    <bean id="loggerByName" class="se.joshua.spring.autowiring.LoggerByName" autowire="byName">
    </bean>

    <bean id="fileWriter" class="se.joshua.spring.autowiring.FileWriter"/>
    <bean id="consoleWriter" class="se.joshua.spring.autowiring.ConsoleWriter"/>


</beans>